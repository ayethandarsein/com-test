package com.panu.competitor.information.dto;

import java.util.List;

/**
 *
 * @author Htet Nanda Kyaw
 */
public class PlantAndCapacityPieData {

    private String name;
    private double y;
    private DataLabels dataLabels;
    private int id;
    private String color;

    public PlantAndCapacityPieData(String name, int y) {
        this.name = name;
        this.y = y;
    }

    public PlantAndCapacityPieData() {
    }
    
    public PlantAndCapacityPieData(int id, String name, double y, DataLabels dataLabels) {
        this.name = name;
        this.y = y;
        this.dataLabels = dataLabels;
        this.id = id;
    }

    public PlantAndCapacityPieData(int id, String name, double y, DataLabels dataLabels, String color) {
        this.name = name;
        this.y = y;
        this.dataLabels = dataLabels;
        this.id = id;
        this.color = color;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public double getY() {
        return y;
    }

    public void setY(double y) {
        this.y = y;
    }

    public DataLabels getDataLabels() {
        return dataLabels;
    }

    public void setDataLabels(DataLabels dataLabels) {
        this.dataLabels = dataLabels;
    }

    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    public String getColor() {
        return color;
    }

    public void setColor(String color) {
        this.color = color;
    }

}
