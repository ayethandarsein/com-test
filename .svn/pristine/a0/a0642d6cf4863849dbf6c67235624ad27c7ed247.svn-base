.noprintfortrucktable{
    display: none!important;
}
@media print {
    @page {
        size: portrait;
    }
    div[id="charthaha:printTable"] thead {
        display:none!important;
    }
    .show{
        display: block!important;
    }
    .nevershowfortruckbtn{
        display: none!important; 
    }

    .nevershowtrucktable{
        display: none!important; 
    }
    .noprintfortrucktable{
        display: block!important;
    }
    header nav, footer {
        display: none!important;
    }
}